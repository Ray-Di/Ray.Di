{
    "name": "ray/di",
    "description": "Guice style annotation-driven dependency injection framework",
    "keywords": [
        "ray",
        "di",
        "aop",
        "annotations",
        "guice"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ray/aop": "^2.4.1",
        "ray/compiler": "^1.1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Ray\\Di\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ray\\Di\\": ["tests/", "tests/Fake/"],
            "FakeVendor\\Sandbox\\": "tests/Fake/FakeVendor/Sandbox/"
        }
    }
}
