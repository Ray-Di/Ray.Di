{
    "name": "ray/di",
    "description": "Guice style annotation-driven dependency injection framework",
    "keywords": [
        "di",
        "aop",
        "annotations",
        "guice"
    ],
    "homepage": "https://github.com/koriym/Ray.Di",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ray/aop": "~2.0",
        "doctrine/cache": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "Ray\\Di\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ray\\Di\\": [
                "tests/",
                "tests/Fake/"
            ],
            "FakeVendor\\Sandbox\\": "tests/Fake/FakeVendor/Sandbox/"
        },
        "files": [
            "tests/annotations.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop-2": "2.0.x-dev"
        }
    }
}
